{
  "expo": {
    "name": "SportSync",
    "slug": "sportsyncapp",
    "description": "Make planning sports events with friends easier with SportSync!",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "sportsyncapp",
    "icon": "./src/assets/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ilyasassakali.sportsyncapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ilyasassakali.sportsyncapp"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "19e375a0-6e89-46ce-8fdb-485e73631b17"
      }
    },
    "owner": "ilyasassakali",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/19e375a0-6e89-46ce-8fdb-485e73631b17"
    }
  }
}
